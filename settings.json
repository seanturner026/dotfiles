{
  "[dockerfile]": {
    "editor.quickSuggestions": {
      "comments": false,
      "other": false,
      "strings": false
    }
  },

  "dev.containers.defaultExtensions": [
    "dakara.transformer",
    "eamodio.gitlens",
    "esbenp.prettier-vscode",
    "feyoung.comment-aligner-space",
    "ms-python.python",
    "ms-python.black-formatter",
    "ms-python.flake8",
    "ms-python.mypy-type-checker",
    // "ms-python.vscode-pylance",
    "rafamel.subtle-brackets",
    "stkb.rewrap",
    "streetsidesoftware.code-spell-checker"
  ],
  "editor.cursorBlinking": "phase",
  "editor.definitionLinkOpensInPeek": true,
  "editor.occurrencesHighlight": "off",
  "editor.matchBrackets": "never",
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 200,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.renderWhitespace": "all",
  "editor.snippetSuggestions": "top",
  "editor.suggestOnTriggerCharacters": false,
  "editor.tabCompletion": "on",
  "editor.quickSuggestions": {
    "strings": true
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,

  "extensions.ignoreRecommendations": true,

  "files.associations": {
    "Dockerfile.*": "dockerfile",
    "*.yaml.gotmpl": "helm",
    "*.tfvars": "terraform",
    "*.hcl": "terraform",
    "*.hujson": "jsonc"
  },
  "json.schemas": [
    {
      "fileMatch": ["*.hujson"],
      "schema": {
        "allowTrailingCommas": true
      }
    }
  ],
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,

  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "[go]": {
    "editor.inlayHints.enabled": "off",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    // Optional: Disable snippets, as they conflict with completion ranking.
    "editor.snippetSuggestions": "none"
  },
  "[go.mod]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "gopls": {
    // Add parameter placeholders when completing a function.
    "usePlaceholders": true,
    // If true, enable additional analyses with staticcheck.
    // Warning: This will significantly increase memory usage.
    "staticcheck": true
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "vscode.json-language-features"
  },

  // "multiCommand.commands": [
  //   {
  //     "command": "multiCommand.saveFileAndSortImportsWhenPython",
  //     "sequence": ["python.sortImports", "workbench.action.files.save"]
  //   }
  // ],

  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.formatOnType": true
  },
  "mypy-type-checker.importStrategy": "fromEnvironment",
  "autoDocstring.docstringFormat": "sphinx-notypes",
  "autoDocstring.startOnNewLine": true,
  "black-formatter.args": ["--config", "pyproject.toml", "--preview"],
  "black-formatter.importStrategy": "fromEnvironment",
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "flake8.args": ["--config=setup.cfg"],
  "flake8.importStrategy": "fromEnvironment",
  // "python.analysis.typeCheckingMode": "strict", // "basic"
  "python.formatting.blackArgs": ["--config", "pyproject.toml", "--preview"],
  "python.languageServer": "Pylance",
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  // "python.formatting.blackPath": "/Users/sean/.local/bin/black",

  "[ruby]": {
    "editor.formatOnSave": true
  },

  "sqltools.highlightQuery": false,
  "terminal.integrated.cwd": "/Users/sean",

  "terraform.languageServer.enable": true,
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "workbench.colorCustomizations": {
    // "activityBar.background": "#1a1d22",
    "editor.background": "#1b1e23",
    // "editorLineNumber.foreground": "#0c79a0",
    // "editorLineNumber.activeForeground": "#555555",
    "editorIndentGuide.activeBackground": "#caced6",
    "editorIndentGuide.background": "#3c4049",
    "statusBar.background": "#0c334d",
    // "statusBar.debuggingBackground": "#263238",
    "statusBar.foreground": "#ffffff",
    // "statusBar.noFolderBackground": "#212121",
    "[Atom One Dark]": {
      // "editor.background": "#1b1e23",
      // "editor.foreground": "#aab2bf",
      "tab.activeBackground": "#1e1f21"
      // "panel.background": "#1e1e1e",
      // "menu.background": "#242426",
      // "terminal.background": "#1e1e1e"
    },
    "editorPane.background": "#1e1e1e"
  },
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.iconTheme": "eq-material-theme-icons",
  "workbench.statusBar.visible": true,
  "workbench.tree.indent": 16,

  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",

  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    // "editor.formatOnSave": true,
    // "editor.insertSpaces": true,
    // "editor.rulers": [120],
    "editor.quickSuggestions": {
      "other": false,
      "comments": false,
      "strings": false
    },
    "editor.tabSize": 2,
    "rewrap.autoWrap.enabled": true
  },
  "yaml.completion": true,
  "yaml.format.printWidth": 120,
  "yaml.hover": true,
  "editor.quickSuggestionsDelay": 100,
  "editor.selectionHighlight": false,
  "workbench.startupEditor": "none",
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "timeline.excludeSources": [],
  "gitlens.codeLens.recentChange.command": false,
  "gitlens.codeLens.authors.command": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.hovers.avatars": false,
  "gitlens.codeLens.enabled": false,
  "go.toolsManagement.autoUpdate": true,
  "python.showStartPage": false,
  "workbench.colorTheme": "One Dark Pro Darker",
  "aws.profile": "profile:default",
  "redhat.telemetry.enabled": false,
  "docker.showStartPage": false,
  "security.workspace.trust.enabled": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "files.trimFinalNewlines": true,
  "editor.glyphMargin": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "rewrap.autoWrap.enabled": false
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false,
    "python": false,
    "terraform": false,
    "go": false
  },
